<div class="large-container">
  <h1> Upcoming Events</h1>
  <% @upcoming.each do |event| %>
    <%= render partial: 'event', locals: { event: event } %>
  <% end %>
</div>
<div class="large-container">
  <h1>Past Events</h1>
  <% @past.each do |event| %>
    <%= render partial: 'event', locals: { event: event } %>
  <% end %>
</div>